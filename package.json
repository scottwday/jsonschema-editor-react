{
  "name": "@optum/json-schema-editor",
  "version": "2.1.0",
  "description": "JsonSchema Editor React Control",
  "repository": "https://github.com/optum/jsonschema-editor-react",
  "license": "Apache 2.0",
  "engines": {
    "node": ">=10.18.0 <11 || >=12.14.0 <13 || >=13.5.0"
  },
  "source": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "react"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "microbundle --jsx React.createElement",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "rimraf docs && storybook dev -p 6006",
    "build-storybook": "rimraf docs && storybook build --docs -o docs"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@hookstate/core": "^4.0.1",
    "bootstrap": "^5.3.3",
    "framer-motion": "^4.0.0",
    "ramda": "^0.27.1",
    "react-bootstrap": "^2.10.6",
    "react-icons": "^3.0.0",
    "typescript": "^4.9.5",
    "use-debounce": "^6.0.1",
    "web-vitals": "^0.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "storybook": "^8.4.7",
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/node-logger": "^8.4.7",
    "@storybook/preset-create-react-app": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-webpack5": "^8.4.7",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.27.40",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "microbundle": "^0.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  }
}
